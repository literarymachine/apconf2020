@base <https://example.org/peertube/categories/>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.


<scheme> a skos:ConceptScheme;
        dct:title "PeerTube Categories"@en, "PeerTube-Kategorien"@de;
        dct:description "The list of categories used in PeerTube software"@en ;
        dct:issued "2020-08-27" ;
        dct:license <http://creativecommons.org/publicdomain/zero/1.0/> ;
        skos:hasTopConcept <Music>, <Vehicles>, <Films>, <Art>, <Sports>, <Travels>, <Gaming>,
            <People>, <Comedy>, <Entertainment>, <NewsPolitics>, <HowTo>, <Education>, <Activism>, 
            <ScienceTechnology>, <Animals>, <Kids>, <Food>, <slide>, <text>, <video>, <web_page>,
            <worksheet>, <other> .

<Music> a skos:Concept;
           skos:topConceptOf <scheme> ;
           skos:prefLabel "Musik"@de, "Music"@en .

<Vehicles> a skos:Concept;
